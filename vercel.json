{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/KashFlowPos",
  "framework": "angular",
  "installCommand": "npm install --include=dev",
  "functions": {
    "api/functions/api/index.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/functions/api/index.ts"
    }
  ]
}
